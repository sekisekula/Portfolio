title = "Michał Sekuła - Data Science Portfolio"
baseURL = "https://sekisekula.github.io/Portfolio"
languageCode = "en-us"
theme = "ananke"
#resourceDir = "../resources"

DefaultContentLanguage = "en"
DefaultContentLanguageInSubdir = false
SectionPagesMenu = "main"
Paginate = 10 # this is set low for demonstrating with dummy content. Set to a higher number # from 3 to 10
googleAnalytics = ""
enableRobotsTXT = true
canonifyURLs = true

[languages]
  [languages.en]
    title = "Michał Sekuła"
    weight = 1
    contentDir = "content/en"
    # languageDirection = 'rtl' for Right-To-Left languages
#  [languages.fr]
#    title = "Ananke Fr"
#    weight = 2
#    contentDir = "content/fr"

[sitemap]
  changefreq = "monthly"
  priority = 0.5
  filename = "sitemap.xml"

[params]
  text_color = ""
  author = ""
  favicon = ""
  site_logo = ""
  description = "The last theme you'll ever need. Maybe."
  github = "https://github.com/SekiSekula"
  instagram = "https://instagram.com/SekiSekula"
  # choose a background color from any on this page: https://tachyons.io/docs/themes/skins/ and preface it with "bg-"
  background_color_class = "bg-black"
  recent_posts_number = 3

[[params.ananke_socials]]
name = "twitter"
url = "https://twitter.com/GoHugoIO"
